<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.infnet.classmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cm"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cm_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/MainTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".utils.ResetarSenha"/>
        <activity android:name=".QuestionComposerActivity" />
        <activity android:name=".QuestionFeedbackActivity" />
        <activity android:name=".ShowQuestionActivity"></activity>
        <activity android:name=".LoginActivity">

        </activity>
        <activity android:name=".SignUpActivity"/>
    </application>

</manifest>